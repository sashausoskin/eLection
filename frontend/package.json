{
	"name": "frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"license": "Apache-2.0",
	"scripts": {
		"dev": "cross-env NODE_ENV=dev vite --host",
		"build": "tsc && vite build",
		"build:production": "tsc && cross-env NODE_ENV=production vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview  --host",
		"test": "vitest run",
		"test:e2e-ui": "cypress open",
		"test:e2e": "cypress run",
		"start:e2e": "npm run build && cross-env NODE_ENV=test npm run preview",
		"sync-translations": "i18next 'src/**/*.{ts,tsx}' [-oc]"
	},
	"dependencies": {
		"@react-spring/web": "^9.7.4",
		"@use-gesture/react": "^10.3.1",
		"axios": "^1.7.2",
		"dotenv": "^16.4.5",
		"formik": "^2.4.6",
		"i18next": "^23.12.2",
		"i18next-browser-languagedetector": "^8.0.0",
		"i18next-http-backend": "^2.5.2",
		"lodash": "^4.17.21",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-i18next": "^15.0.0",
		"react-router": "^6.23.1",
		"react-router-dom": "^6.23.1",
		"react-tooltip": "^5.27.1",
		"socket.io-client": "^4.7.5",
		"yup": "^1.4.0"
	},
	"devDependencies": {
		"@4tw/cypress-drag-drop": "^2.2.5",
		"@tanstack/eslint-plugin-query": "^5.35.6",
		"@testing-library/dom": "^10.1.0",
		"@testing-library/jest-dom": "^6.4.5",
		"@testing-library/react": "^16.0.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^29.5.12",
		"@types/lodash": "^4.17.7",
		"@types/mocha": "^10.0.7",
		"@types/node": "^20.14.0",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.13.0",
		"@typescript-eslint/parser": "^7.13.0",
		"@vitejs/plugin-react": "^4.2.1",
		"cross-env": "^7.0.3",
		"cypress": "^13.13.0",
		"cypress-real-events": "^1.13.0",
		"eslint": "^8.57.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"i18next-parser": "^9.0.0",
		"identity-obj-proxy": "^3.0.0",
		"jest-environment-jsdom": "^29.7.0",
		"jsdom": "^24.1.0",
		"prettier": "^3.3.2",
		"typescript": "^5.2.2",
		"vite": "^5.2.0",
		"vitest": "^2.0.4"
	}
}
